{
  "name": "expenses",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --runInBand",
    "test:watch": "jest --watch --runInBand",
    "test:cov": "jest --coverage --runInBand",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db:drop": "npx typeorm schema:drop -d datasource.js",
    "db:migrate": "ts-node --transpile-only scripts/migrate.ts",
    "db:migrate:docker_prod": "node dist/scripts/migrate.js",
    "db:migrate:new": "typeorm migration:generate -p -d datasource.js ./migrations/default/init",
    "db:migrate:empty": "npx typeorm migration:create"
  },
  "dependencies": {
    "@fastify/static": "9.0.0",
    "@grpc/grpc-js": "1.14.3",
    "@grpc/proto-loader": "0.8.0",
    "@nestjs/axios": "4.0.1",
    "@nestjs/common": "11.1.14",
    "@nestjs/config": "4.0.3",
    "@nestjs/core": "11.1.14",
    "@nestjs/microservices": "11.1.14",
    "@nestjs/platform-fastify": "11.1.14",
    "@nestjs/schedule": "6.1.1",
    "@nestjs/swagger": "11.2.6",
    "@nestjs/terminus": "11.1.0",
    "@nestjs/typeorm": "11.0.0",
    "@opentelemetry/api": "1.9.0",
    "@opentelemetry/auto-instrumentations-node": "0.70.0",
    "@opentelemetry/exporter-metrics-otlp-grpc": "0.212.0",
    "@opentelemetry/exporter-trace-otlp-grpc": "0.212.0",
    "@opentelemetry/instrumentation": "0.212.0",
    "@opentelemetry/resources": "2.5.1",
    "@opentelemetry/sdk-node": "0.212.0",
    "async-retry": "1.3.3",
    "axios": "1.13.5",
    "bcrypt": "6.0.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.3",
    "dotenv": "17.3.1",
    "pg": "8.18.0",
    "reflect-metadata": "0.2.2",
    "tsconfig-paths": "4.2.0",
    "typeorm": "0.3.28",
    "typeorm-naming-strategies": "4.1.0",
    "ulid": "3.0.2",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.3",
    "@eslint/js": "9.39.2",
    "@nestjs/cli": "11.0.16",
    "@nestjs/schematics": "11.0.9",
    "@nestjs/testing": "11.1.14",
    "@types/async-retry": "1.4.9",
    "@types/bcrypt": "6.0.0",
    "@types/jest": "30.0.0",
    "@types/lodash": "4.17.23",
    "@types/node": "25.2.3",
    "@types/supertest": "6.0.3",
    "@typescript-eslint/eslint-plugin": "8.56.0",
    "@typescript-eslint/parser": "8.56.0",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.5",
    "fastify": "5.7.4",
    "globals": "17.3.0",
    "jest": "30.2.0",
    "prettier": "3.8.1",
    "source-map-support": "0.5.21",
    "supertest": "7.2.2",
    "ts-jest": "29.4.6",
    "ts-loader": "9.5.4",
    "ts-node": "10.9.2",
    "typescript": "5.9.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".(test|spec|e2e).ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "testTimeout": 20000,
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "#api/(.*)": "<rootDir>/api/$1",
      "#domain/(.*)": "<rootDir>/domain/$1",
      "#usecases/(.*)": "<rootDir>/usecases/$1",
      "#frameworks/(.*)": "<rootDir>/frameworks/$1",
      "#packages/(.*)": "<rootDir>/packages/$1"
    }
  }
}
